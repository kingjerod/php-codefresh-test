{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "ryze-api",
      "image": "025705197927.dkr.ecr.us-west-2.amazonaws.com/ryze/ryze-api",
      "environment": [
        {
          "name": "Container",
          "value": "PHP"
        }
      ],
      "essential": true,
      "memory": 128
    },
    {
      "name": "ryze-nginx",
      "image": "025705197927.dkr.ecr.us-west-2.amazonaws.com/ryze/ryze-nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "ryze-api"
      ],
      "volumesFrom": [
        {
          "sourceContainer": "ryze-api"
        }
      ]
    }
  ]
}
